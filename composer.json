{
    "name": "bluedogtraining/avetmiss",
    "description": "AVETMISS Validation and Report Generation",
    "keywords": [
        "avetmiss"
    ],
    "homepage": "https://github.com/bluedogtraining/avetmiss",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Senn",
            "email": "tech@bluedogtraining.com.au",
            "homepage": "http://bluedogtraining.com.au/"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "zendframework/zendframework1": "1.*",
        "illuminate/validation": "5.*|6.*|7.*"
    },
    "autoload": {
        "psr-4": { 
            "Bdt\\Avetmiss\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bdt\\Avetmiss\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "zendframework/zendframework1": "Allows Zend Framework 1 validation integration",
        "illuminate/validation": "Allows Laravel 5 validation integration"
    },
    "scripts": {
        "test": "phpunit"
    }
